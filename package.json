{
  "name": "projet",
  "version": "1.0.0",
  "description": "Bot Discord New Heaven of Gaming",
  "main": "main.js",
  "repository": "https://github.com/Raeskinet/Bot-discord-NHG.git",
  "author": "Raeskinet <contact@raeskinet.com>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^12.0.2",
    "mongoose": "^5.9.7"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-aqua": "^7.2.1"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off",
      "no-trailing-spaces": "off",
      "prefer-template": "off",
      "no-eval": "off"
    }
  }
}
